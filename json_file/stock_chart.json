{
  "type": "bubble",
  "header": {
    "type": "box",
    "layout": "vertical",
    "contents": [
      {
        "type": "text",
        "text": "Stock Chart",
        "align": "center",
        "size": "xl"
      }
    ]
  },
  "body": {
    "type": "box",
    "layout": "vertical",
    "contents": [
      {
        "type": "button",
        "action": {
          "type": "message",
          "label": "1 Minute",
          "text": "1m"
        }
      },
      {
        "type": "button",
        "action": {
          "type": "message",
          "label": "5 Minute",
          "text": "5m"
        }
      },
      {
        "type": "button",
        "action": {
          "type": "message",
          "label": "30 Minute",
          "text": "30m"
        }
      },
      {
        "type": "button",
        "action": {
          "type": "message",
          "label": "1 Hour",
          "text": "1h"
        }
      },
      {
        "type": "button",
        "action": {
          "type": "message",
          "label": "1 Day",
          "text": "1d"
        }
      },
      {
        "type": "button",
        "action": {
          "type": "message",
          "label": "1 Week",
          "text": "1wk"
        }
      },
      {
        "type": "button",
        "action": {
          "type": "message",
          "label": "1 Month",
          "text": "1mo"
        }
      },
      {
        "type": "button",
        "action": {
          "type": "message",
          "label": "3 Month",
          "text": "3mo"
        }
      },
      {
        "type": "button",
        "action": {
          "type": "message",
          "label": "All Time",
          "text": "all time"
        }
      }
    ]
  },
  "footer": {
    "type": "box",
    "layout": "horizontal",
    "contents": [
      {
        "type": "button",
        "action": {
          "type": "message",
          "label": "Back",
          "text": "back"
        }
      },
      {
        "type": "button",
        "action": {
          "type": "message",
                  "label": "Menu",
                  "text": "menu"
        }
      }
    ]
  },
  "styles": {
    "header": {
      "backgroundColor": "#F0997E"
    },
    "body": {
      "backgroundColor": "#F3EFEC"
    },
    "footer": {
      "backgroundColor": "#DAD6D3"
    }
  }
}